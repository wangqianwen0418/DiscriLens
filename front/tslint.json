{
  "extends": ["tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "allowJs": true,
  "jsRules": {
    "no-empty": true
  },
  "rules":{
    "no-unused-variable": false,
    "no-console": false
  },
  "defaultSeverity": "warning"
}
